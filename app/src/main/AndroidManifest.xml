<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.registrationformapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Waste Management Service"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RegistrationFormApp">
        <activity
            android:name=".Collect_My_Waste"
            android:exported="true"
            android:label="Collect My Waste"
            android:parentActivityName=".SummaryActivity"/>
        <activity
            android:name=".Metallic_Waste"
            android:exported="true"
            android:parentActivityName=".Collect_My_Waste"
            android:label="Your Waste Details"/>
        <activity
            android:name=".CoinsCollected"
            android:exported="true"
            android:label="Kaustav Industries"
            android:parentActivityName=".SummaryActivity" />
        <activity
            android:name=".MainActivity2"
            android:exported="true"
            android:label="User LogIn" />
        <activity
            android:name=".SummaryActivity"
            android:exported="true"
            android:label="User Account" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>